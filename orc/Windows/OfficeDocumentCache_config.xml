<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <!--Documentation-->
    <!--https://arsenalrecon.com/2019/10/the-office-document-cache-and-introducing-odc-recon-part-i/-->
    <sample name="Office_Document_Cache">
        <ntfs_find path_match="Users\*\AppData\Local\Microsoft\Office\*\OfficeFileCache\"/>
    </sample>
</getthis>
