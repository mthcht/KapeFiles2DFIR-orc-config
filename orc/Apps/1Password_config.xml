<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <!--Documentation-->
    <!--https://blog.elcomsoft.com/2017/08/attacking-the-1password-master-password-follow-up/-->
    <sample name="1Password_Database">
        <ntfs_find path_match="Users\*\AppData\Local\1password\data1Password10.sqlite"/>
    </sample>
    <sample name="1Password_Backup_Databases">
        <ntfs_find path_match="Users\*\AppData\Local\1password\backups1Password10.sqlite"/>
    </sample>
    <sample name="1Password_Logs">
        <ntfs_find path_match="Users\*\AppData\Local\1password\logs*.log"/>
    </sample>
</getthis>
