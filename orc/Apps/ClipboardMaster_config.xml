<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <!--Documentation-->
    <!--Clipboard Master is a universal multi-clipboard for text, images, files, etc for Windows XP+.-->
    <!--Clipboard.clm4 can be viewed in any text editor. It appears to store in plaintext the userâ€™s clipboard history.-->
    <!--Clipboard.clm.bak appears to be a 1:1 backup of Clipboard.clm4.-->
    <!--There appeared to be multiple files with the naming convention ofClipboard.clm4.ba# where # iterated from 2 upward. These were also plaintext readable in any text editor.-->
    <sample name="ClipboardMaster_-_Clipboard_History_-_Text">
        <ntfs_find path_match="Users\*\AppData\Roaming\Jumping Bytes\ClipboardMaster\Clipboard.clm4"/>
    </sample>
    <sample name="ClipboardMaster_-_Clipboard_History_-_Images">
        <ntfs_find path_match="Users\*\AppData\Roaming\Jumping Bytes\ClipboardMaster\pics\"/>
    </sample>
    <sample name="ClipboardMaster_-_Clipboard_History_-_Backups">
        <ntfs_find path_match="Users\*\AppData\Roaming\Jumping Bytes\ClipboardMaster\Clipboard.clm4.ba*"/>
    </sample>
</getthis>
