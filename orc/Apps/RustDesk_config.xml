<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <!--Documentation-->
    <!--https://github.com/rustdesk/rustdesk/wiki/FAQ#access-logs-->
    <sample name="RustDesk_logs">
        <ntfs_find path_match="Users\*\AppData\Roaming\RustDesk\"/>
    </sample>
    <sample name="RustDesk_logs">
        <ntfs_find path_match="Windows\ServiceProfiles\LocalService\AppData\Roaming\RustDesk\log\server"/>
    </sample>
</getthis>
