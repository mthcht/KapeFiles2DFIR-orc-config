<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>*</location>
    <!--Documentation-->
    <!--https://az4n6.blogspot.com/2020/02/detecting-laterial-movment-with-winscp.html for more information-->
    <!--https://www.fireeye.com/blog/threat-research/2017/03/using_the_registryt.html-->
    <!--This will grab the .ini file which is a configuration file that can provide details about the FTP session-->
    <!--This file should be located in the same folder as WinSCP.exe-->
    <sample name="WinSCP_(.ini_file)">
        <ntfs_find name="WinSCP.ini"/>
    </sample>
</getthis>
