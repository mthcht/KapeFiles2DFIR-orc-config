<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <!--Documentation-->
    <!--A target to run on systems that may be hosting web servers. Helpful in determining whether web application exploitation is a contributing factor.-->
    <sample name="Apache_Access_Logs">
        <ntfs_find path_match="ApacheAccessLog.tkape"/>
    </sample>
    <sample name="IIS_Logs">
        <ntfs_find path_match="IISLogFiles.tkape"/>
    </sample>
    <sample name="NGINX_Logs">
        <ntfs_find path_match="NGINXLogs.tkape"/>
    </sample>
    <sample name="MSSQL_Error_Logs">
        <ntfs_find path_match="MSSQLErrorLog.tkape"/>
    </sample>
</getthis>
